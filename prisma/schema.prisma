generator client {
     provider = "prisma-client-js"
}

datasource db {
     provider = "mysql"
     url = env("DATABASE_URL")
}

model AboutMe {
     userId BigInt @id
     pronouns String @db.VarChar(60)
     bio String @db.VarChar(255)
}

model StrikeCase {
     id Int @id @default(autoincrement())
     userId BigInt
     reason String
     timestamp DateTime
     messageLink String?
     deleted Boolean @default(false)
}